<script type="text/javascript">

   
	function aplicafiltres (filtre,vista) {
		var filtres = ["mostraDocent","mostraEstudiant","mostraTotes"];
	
		var filtrat = [];
		filtrat.push(filtre);
		var restef = [];
		$.grep(filtres, function(el) {
			if (jQuery.inArray(el, filtrat) == -1) restef.push(el);
		});
	
		$.each(restef, function( index, value ) {
			$("."+value).removeClass('actiu');
		});
	
		$("."+filtre).addClass('actiu');
	
		if (filtre=="mostraTotes") {
			$(".panellTeacher").show();
			$(".panellStudent").show();
		}
	
		if (filtre=="mostraDocent") {
			$(".panellTeacher").show();
			$(".panellStudent").hide();
		}
	
		if (filtre=="mostraEstudiant") {
			$(".panellTeacher").hide();
			$(".panellStudent").show();
		}
	
		if (vista=="caixes") {
			$("#llistat-aules").removeClass('vista-simplificada');
		   }
	
		if (vista=="llistat") {
			$("#llistat-aules").addClass('vista-simplificada');
		}
	
	
		$('#prof').change(function() {
			// this will contain a reference to the checkbox   
			if (this.checked) {
				// the checkbox is now checked 
					$(".panellTeacher").show();
					 localStorage['profe']="1";
			} else {
				$(".panellTeacher").hide();
				localStorage['profe']="0";
			}
		});
	
		$('#est').change(function() {
			// this will contain a reference to the checkbox   
			if (this.checked) {
				// the checkbox is now checked
				$(".panellStudent").show(); 
				localStorage['alumne'] = "1";
			} else {
				$(".panellStudent").hide();
				localStorage['alumne'] = "0";
			}
		});
	
	
	};
	
		function aplicavista (vista) {
				if (vista=="caixes") {
						$("#llistat-aules").removeClass('vista-simplificada');
						$("#vista-completa").prop('checked',true); 
				   }
	
				if (vista=="llistat") {
						$("#llistat-aules").addClass('vista-simplificada');
						$("#vista-simplificada").prop('checked',true);  
				}    			
		};
	
		function aplicaedicio (edita) {
				if (edita=="1") {
					$(".desplaca-aula").toggleClass("off");
					$("#organitza-aules").attr("checked", true);
				  }
	
		};
	
		function aplicaavisos (avisos) {
				if (avisos=="1") {
					$("#avisos").removeClass("off");
					$("#avisos").addClass("on");
					$("#mostra-avisos").attr("checked", true);
				  }	
				if (avisos=="0") {
					$("#avisos").removeClass("on");
					$("#avisos").addClass("off");
					$("#mostra-avisos").attr("checked", false);
				  }					
		};
	
	function aaaaa(elem) {
	
		Dropdowns.init();
	
	};
	
	$(document).ready(function() {
		var sivista = localStorage['vista'];
		var siprofe = localStorage['profe'];
		var sialumne = localStorage['alumne'];
		var siedita = localStorage['edita'];
		var siavisos = localStorage['avisos'];
	
		if (!sivista) {
			localStorage['vista'] = "caixes";
		}
	
		if (!siprofe) {
			localStorage['profe'] = "1";
		}
	
		if (!sialumne) {
			localStorage['alumne'] = "1";
		}
	
		if (!siedita) {
			localStorage['edita'] = "0";
		}
	
		if (!siavisos) {
			localStorage['avisos'] = "1";
		}
	
		$('#prof').change(function() {
			// this will contain a reference to the checkbox   
			if (this.checked) {
				// the checkbox is now checked 
					$(".panellTeacher").show();
					 localStorage['profe']="1";
			} else {
				$(".panellTeacher").hide();
				localStorage['profe']="0";
			}
		});
	
		$('#est').change(function() {
			// this will contain a reference to the checkbox   
			if (this.checked) {
				// the checkbox is now checked
				$(".panellStudent").show(); 
				localStorage['alumne'] = "1";
			} else {
				$(".panellStudent").hide();
				localStorage['alumne'] = "0";
			}
		});
	
		$("#organitza-aules").change(function(){
			
			if (localStorage['edita'] == "1") {
				$(".desplaca-aula").addClass("off");
				localStorage['edita'] = "0";
			} else  {
				$(".desplaca-aula").removeClass("off");
				localStorage['edita'] = "1";
	
			}	
		}); 
	
		$("#mostra-avisos").change(function(){
			if (localStorage['avisos'] == "0") {			
				$("#avisos").addClass("on");
				$("#avisos").removeClass("off");
				localStorage['avisos'] = "1";
			} 
			else  {
				$("#avisos").addClass("off");
				$("#avisos").removeClass("on");
				localStorage['avisos'] = "0";
			}
		}); 
	
		$('.mostraTotes').click(function() {
			$(".mostraDocent").removeClass('actiu');
			$(".mostraEstudiant").removeClass('actiu');
			$(".panellTeacher").show();
			$(".panellStudent").show();
			$(this).addClass("actiu");					
		});
	
		$('.mostraDocent').click(function() {
			$(".mostraTotes").removeClass('actiu');
			$(".mostraEstudiant").removeClass('actiu');
			$(".panellTeacher").show();					
			$(".panellStudent").hide();
			$(this).addClass('actiu');
		});
	
	
		$('.mostraEstudiant').click(function() {
			$(".mostraTotes").removeClass('actiu');
			$(".mostraDocent").removeClass('actiu');					
			$(".panellTeacher").hide();
			$(".panellStudent").show();
			$(this).addClass('actiu');
		});
	
		$('#vista-simplificada').click(function() {
			$("#llistat-aules").addClass('vista-simplificada');
			localStorage['vista'] = "llistat";
		});
	
		$('#vista-completa').click(function() {
			$("#llistat-aules").removeClass('vista-simplificada');
			localStorage['vista'] = "caixes";		
		});
	
		
		   $('.activar-aula').click(function() {		
			var idactivar = $(this).attr("id");
			$("#aelements").attr("value",idactivar.replace(/a/gi,''));
			var vista=$("body").attr("id");
			$("#vista").attr("value",vista);
			$("#formActiva").attr("action","execActivarAula.jsp");
			$("#formActiva").submit();	
			
		});
	
		   $('.desactivar-aula').click(function() {		
			var iddesactivar = $(this).attr("id");
			$("#aelements").attr("value",iddesactivar.replace(/d/gi,''));
			var vista=$("body").attr("id");
			$("#vista").attr("value",vista);		
			$("#formActiva").attr("action","execDesactivarAula.jsp");	
			$("#formActiva").submit();	
			
		});
	
	
		aplicavista(localStorage["vista"]);
		aplicaedicio(localStorage["edita"]);
		aplicaavisos(localStorage["avisos"]);
	
		var param = {
			"niu": "1620066",
			"ideds": "MO71836,MO68794,MO69238,MO69421,MO73011"
		};
	
		var paramcvmoo = {
			"niu": "1620066",
			"ideds": "MO71985"
		};
	
		var activitats = [
			{key: "feedback", nom: "Retroacció", nou:"nova",act:"actualitzada"},
			{key: "lesson", nom: "Lliçó", nou:"nova",act:"actualitzada"},
			{key: "assign", nom: "Tasca", nou:"nova",act:"actualitzada"},
			{key: "choice", nom: "Consulta", nou:"nova",act:"actualitzada"},
