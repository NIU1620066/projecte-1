			{key: "groupselect", nom: "Tria de grup", nou:"nova",act:"actualitzada"},
			{key: "wiki", nom: "Wiki", nou:"nova",act:"actualitzada"},
			{key: "data", nom: "Base de dades", nou:"nova",act:"actualitzada"},
