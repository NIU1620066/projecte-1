			{key: "activequiz", nom: "Qüestionari interactiu", nou:"nou",act:"actualitzat"},
			{key: "customcert", nom: "Certificat personalitzat", nou:"nou",act:"actualitzat"},
			{key: "imscp", nom: "Paquet de contingut IMS", nou:"nou",act:"actualitzat"},
			{key: "scorm", nom: "Paquet SCORM", nou:"nou",act:"actualitzat"},
			{key: "quiz", nom: "Qüestionari", nou:"nou",act:"actualitzat"},
