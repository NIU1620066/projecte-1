			{key: "label", nom: "Etiqueta", nou:"nova",act:"actualitzada"},
			{key: "url", nom: "URL", nou:"nova",act:"actualitzada"},
			{key: "lti", nom: "Eina externa", nou:"nova",act:"actualitzada"},
			{key: "scheduler", nom: "Agenda", nou:"nova",act:"actualitzada"},
			{key: "kalvidassign", nom: "Tasca de vídeo de Kaltura",nou:"nova",act:"actualitzada"},
			{key: "news", nom: "Fòrum d'anuncis", nou:"nou",act:"actualitzat"},
