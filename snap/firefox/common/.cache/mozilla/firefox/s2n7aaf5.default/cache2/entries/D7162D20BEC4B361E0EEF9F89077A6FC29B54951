			{key: "kalvidres", nom: "Recurs de vídeo de Kaltura", nou:"nou",act:"actualitzat"}
		]
	
		$.ajax({
			type: 'post',
			dataType: 'json',
			url: 'https://e-aules.uab.cat/2024-25/apsi/novetats.php',
			data: param,
			success: function(data) { 
				var items = [];
	
				$.each( data, function( key, val ) {
					var novetats = 0;
					if (val.curs_existeix == '0')
					{
						$("#n"+val.ided).html("<p style='height:75px;'>No hi ha dades a mostrar. Contacteu amb el CAS (Centre d'Assistència i Suport) <a href='mailto:cas@uab.cat'>cas@uab.cat</a>.</p>");
					}
	
					if (val.curs_existeix == '1' && val.inscrit == '0')
					{
						$("#p"+val.ided ).html("<p style='height:75px;'>Les novetats es mostraran a partir que accediu per primer cop a l'aula.</p>");
					} 
					if (val.curs_existeix == '1' && val.inscrit == '1')
					{					
						if(val.events_num>0) { 
							novetats=1;
							$("#n"+val.ided).html("<li><a href='' target='_blank' id='esde_ref"+val.ided+"'>Esdeveniments propers</a> <span class='etiqueta positiu' id='n_esde"+val.ided+"'></span></li>");						
						}	
						if(val.events_num==0) {
							$("#n"+val.ided).html("<li><a href='' target='_blank' id='esde_ref"+val.ided+"'>Esdeveniments propers</a> <span class='etiqueta negatiu' id='n_esde"+val.ided+"'></span></li>");
						}
						$("#n_esde"+val.ided).html(val.events_num+" "+"nous" );									    
						$("#esde_ref"+val.ided).attr( 'href', val.events_url );
	
						if($("#n"+val.ided).attr('tipus')=='Standardold')
						{
							if (val.activitats_numero>0) {
								$("#n"+val.ided).append("<li class='novetats-material' id='m"+val.ided+"'><a title='Feu clic per veure i accedir als últims 5 elements publicats o modificats'>Material i activitats</a><button type='button' id='nou-material-boto-A"+val.ided+"' class='boto petit invertit' data-component='dropdown' data-target='ma"+val.ided+"' >"+val.activitats_numero+" nous<svg focusable='false' aria-hidden='true' viewbox='0 0 24 24' class='icona dropdown'><use href='img/icones.svg#icona-dropdown'></use></svg></button></li>");
								novetats=1;
							}
							if (val.activitats_numero==0) {
								$("#n"+val.ided).append("<li class='novetats-material' id='m"+val.ided+"'><a href='javascript:void(0);'>Material i activitats<span class='etiqueta negatiu'> 0 nous</span></a></li>");
							}
	
							$("#m"+val.ided).append("<ul id='ma"+val.ided+"' class='desplegable' aria-hidden='true'>");										
							$.each( val.modules, function( key, modul ) {
	
								var nom=" ";	
								var nou="nou";
								var act="actualitzat";
								$.each(activitats, function() { 
									 if(this.key === modul.tipus){ 
										   nom=this.nom; 
										nou=this.nou; 
										act=this.act;							     
									 } 
								}); 
	
								if (modul.estat=='updated') { $("#ma"+val.ided).append("<li data-item><a class='desplegable-item "+modul.tipus+"' data-focus href='"+modul.url+"' target='_blank'><span class='actualitzat'>"+nom+" "+act+"</span> "+modul.nom+"</a></li>");	}
								if (modul.estat=='new')     { $("#ma"+val.ided).append("<li data-item><a class='desplegable-item "+modul.tipus+"' data-focus href='"+modul.url+"' target='_blank'><span class='nou'>"+nom+" "+nou+"</span> "+modul.nom+"</a></li>");	}
							});
							
							if (val.activitats_numero>5) {
								$("#ma"+val.ided).append("<li data-item><a class='desplegable-item accedir-aula' data-focus href='https://e-aules.uab.cat/2024-25/course/view.php?idnumber="+val.ided+"' target='_blank'><svg focusable='false' aria-hidden='true' viewbox='0 0 24 24' class='icona ves'><use href='img/icones.svg#icona-ves'></use></svg>Accedir a l'aula<span class='sr-only'>"+val.ided+"</span></a></li>");
							}
	
						}	
	
						if($("#n"+val.ided).attr('tipus')=='NoUsers')
						{
							if (val.activitats_numero>0) {
								$("#n"+val.ided).append("<li class='novetats-material' id='m"+val.ided+"'><a title='Feu clic per veure i accedir als últims 5 elements publicats o modificats'>Material i activitats</a>	<button type='button' id='nou-material-boto-A"+val.ided+"' class='boto petit invertit' data-component='dropdown' data-target='ma"+val.ided+"' >"+val.activitats_numero+" nous<svg focusable='false' aria-hidden='true' viewbox='0 0 24 24' class='icona dropdown'><use href='img/icones.svg#icona-dropdown'></use></svg></button></li>");
								novetats=1;
							}
							if (val.activitats_numero==0) {
								$("#n"+val.ided).append("<li class='novetats-material' id='m"+val.ided+"'><a href='javascript:void(0);'>Documents<span class='etiqueta negatiu'> 0 nous</span></a></li>");
							}
	
							$("#m"+val.ided).append("<ul id='ma"+val.ided+"' class='desplegable' aria-hidden='true'>");										
							$.each( val.modules, function( key, modul ) {
	
								var nom=" ";	
								var nou="nou";
								var act="actualitzat";
								$.each(activitats, function() { 
									 if(this.key === modul.tipus){ 
										   nom=this.nom; 
										nou=this.nou; 
										act=this.act;							     
									 } 
								}); 
	
								if (modul.estat=='updated') { $("#ma"+val.ided).append("<li data-item><a class='desplegable-item "+modul.tipus+"' data-focus href='"+modul.url+"' target='_blank'><span class='actualitzat'>"+nom+" "+act+"</span> "+modul.nom+"</a></li>");	}
								if (modul.estat=='new')     { $("#ma"+val.ided).append("<li data-item><a class='desplegable-item "+modul.tipus+"' data-focus href='"+modul.url+"' target='_blank'><span class='nou'>"+nom+" "+nou+"</span> "+modul.nom+"</a></li>");	}
							});
	
							if (val.activitats_numero>5) {
								$("#ma"+val.ided).append("<li data-item><a class='desplegable-item accedir-aula' data-focus href='https://e-aules.uab.cat/2024-25/course/view.php?idnumber="+val.ided+"' target='_blank'><svg focusable='false' aria-hidden='true' viewbox='0 0 24 24' class='icona ves'><use href='img/icones.svg#icona-ves'></use></svg>Accedir a l'aula<span class='sr-only'>"+val.ided+"</span></a></li>");
							}  	
						}		
	
						if (val.noticies_numeroda =='0')
						{
							$("#n"+val.ided).append("<li><a href="+val.noticies_url+" target='_blank'>Avisos i notícies</a><span class='etiqueta negatiu'>"+val.noticies_numeroda+" nous</span></li>");									     	
						}
						else
						{
							novetats=1;
							if ( val.noticies_existeix == '1' && val.noticies_numeronl == '-1') {					
								$("#n"+val.ided).append("<li><a href="+val.noticies_url+" target='_blank'>Avisos i notícies</a><span class='etiqueta positiu'>"+val.noticies_numeroda+" nous</span></li>");							
							}
							else {
								$("#n"+val.ided).append("<li><a href="+val.noticies_url+" target='_blank'>Avisos i notícies</a><span class='etiqueta positiu'>"+val.noticies_numeroda+" nous, "+val.noticies_numeronl+" No llegits</span></li>");
							}
						}
	
						if (novetats==1) {
							//$("#"+val.ided).parent().parent().find("h3").addClass("novetats tooltip");
							//$("#"+val.ided).parent().parent().find("h3").attr("title","Hi ha novetats!");
						}
					} // if
				}); // $.each
				aaaaa(this);
			}						
		});
	
		$.ajax({
			type: 'post',
			dataType: 'json',
			url: 'https://e-aulesi.uab.cat/2024-25/apsi/novetats.php',
			data: paramcvmoo,
			success: function(data) { 
				var items = [];
	
				$.each( data, function( key, val ) {
					var novetats = 0;
					if (val.curs_existeix == '0')
					{
						$("#n"+val.ided).html("<p style='height:75px;'>No hi ha dades a mostrar. Contacteu amb el CAS (Centre d'Assistència i Suport) <a href='mailto:cas@uab.cat'>cas@uab.cat</a>.</p>");
					}
	
					if (val.curs_existeix == '1' && val.inscrit == '0')
					{
						$("#p"+val.ided ).html("<p style='height:75px;'>Les novetats es mostraran a partir que accediu per primer cop a l'aula.</p>");
					} 
					if (val.curs_existeix == '1' && val.inscrit == '1')
					{					
						if(val.events_num>0) { 
							novetats=1;
							$("#n"+val.ided).html("<li><a href='' target='_blank' id='esde_ref"+val.ided+"'>Esdeveniments propers</a> <span class='etiqueta positiu' id='n_esde"+val.ided+"'></span></li>");						
						}	
						if(val.events_num==0) {
							$("#n"+val.ided).html("<li><a href='' target='_blank' id='esde_ref"+val.ided+"'>Esdeveniments propers</a> <span class='etiqueta negatiu' id='n_esde"+val.ided+"'></span></li>");
						}
						$("#n_esde"+val.ided).html(val.events_num+" "+"nous" );									    
						$("#esde_ref"+val.ided).attr( 'href', val.events_url );
	
						if($("#n"+val.ided).attr('tipus')=='Standardold')
						{
							if (val.activitats_numero>0) {
								$("#n"+val.ided).append("<li class='novetats-material' id='m"+val.ided+"'><a title='Feu clic per veure i accedir als últims 5 elements publicats o modificats'>Material i activitats</a><button type='button' id='nou-material-boto-A"+val.ided+"' class='boto petit invertit' data-component='dropdown' data-target='ma"+val.ided+"' >"+val.activitats_numero+" nous<svg focusable='false' aria-hidden='true' viewbox='0 0 24 24' class='icona dropdown'><use href='img/icones.svg#icona-dropdown'></use></svg></button></li>");
								novetats=1;
							}
							if (val.activitats_numero==0) {
								$("#n"+val.ided).append("<li class='novetats-material' id='m"+val.ided+"'><a href='javascript:void(0);'>Material i activitats<span class='etiqueta negatiu'> 0 nous</span></a></li>");
